{
  "name": "expand-value",
  "version": "1.0.0",
  "description": "Expand deeply nested values from an object, with support for advanced features.",
  "repository": "jonschlinkert/expand-value",
  "bugs": {
    "url": "https://github.com/jonschlinkert/expand-value/issues"
  },
  "homepage": "https://github.com/jonschlinkert/expand-value",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "funding": {
    "url": "https://github.com/sponsors/jonschlinkert"
  },
  "license": "MIT",
  "scripts": {
    "eslint": "npx eslint --ext .ts .",
    "cover": "nyc --reporter=text --reporter=html mocha",
    "test": "ts-mocha -r esbuild-register 'test/**/*.ts'",
    "tsup": "npx tsup"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "is-number": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "gulp-format-md": "^2.0.0",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "accessor",
    "dot-prop",
    "expand-value",
    "get",
    "get-value",
    "key",
    "nested",
    "object",
    "path",
    "paths",
    "prop",
    "properties",
    "property",
    "props",
    "resolve-value",
    "segment",
    "set-value",
    "value",
    "values"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "dot-prop",
      "get-value"
    ]
  }
}
